<!-- cocktails/new -->
<h1> Make a New Cocktail! </h1>
<div class="banner" style="height: 400px; background-image: url(http://bearfootbistro.com/wp-content/uploads/2012/11/banner-menu-cocktail.jpg);"></div

<%= simple_form_for(@cocktail)do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
<% end %>